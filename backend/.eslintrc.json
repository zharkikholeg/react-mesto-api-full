{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle": [
            1,
            {
                "allow": [
                    "_id"
                ]
            }
        ],
        "no-console": 0,
        "no-shadow": 0,
        "consistent-return": 0,
        "no-useless-escape": 0,
        "no-unused-vars": 0
    },
    "extends": "airbnb-base"
}